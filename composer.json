{
  "name": "derian-cordoba/laravel-bump-version",
  "description": "Simple library to manage versions in Laravel applications.",
  "homepage": "https://github.com/derian-cordoba/laravel-bump-version",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0",
  "keywords": [
    "version",
    "bump",
    "laravel-version",
    "laravel"
  ],
  "autoload": {
    "psr-4": {
      "BumpVersion\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "derian-cordoba",
      "email": "derianricardo451@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "BumpVersion\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "BumpVersion": "BumpVersion\\Facades\\BumpVersion"
      }
    }
  },
  "scripts": {
    "test": "phpunit tests"
  }
}
